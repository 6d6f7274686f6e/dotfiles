#!/bin/zsh

source $XDG_CACHE_HOME/wal/colors.sh

cat $XDG_CONFIG_HOME/dunst/.template | sed "s/color0/$color0/" \
  | sed "s/color1/$color1/" \
  | sed "s/color2/$color2/" \
  | sed "s/color3/$color3/" \
  | sed "s/color4/$color4/" \
  | sed "s/color5/$color5/" \
  | sed "s/color6/$color6/" \
  | sed "s/color7/$color7/" 
